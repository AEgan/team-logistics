		<ul>
			<li>link to teams</li>
			<%= title %>
			<% if(current_user) { %>
			<%= JSON.stringify(current_user)%>
			<% } else { %>
			<p>Not logged in</p>
			<% } %>
		</ul>
	</body>
</html>